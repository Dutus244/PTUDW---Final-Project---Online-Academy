<div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
  <div class="carousel-inner">
    <div class="carousel-item active">
        <div class="d-block w-100" style="text-align: center; background-color:black">
      <img src="/public/img/category/yae.png" height="300" alt="First slide">
      <div class="carousel-caption d-none d-md-block">
        <span class="Featured">{{first.CourseName}}</span>
        <br>
        <p class="rating-item">{{first.Rating}}</p>
        <i class="bi bi-star-fill"></i>
      </div>
        </div>
    </div>
    <div class="carousel-item">
        <div class="d-block w-100" style="text-align: center; background-color:black">
      <img src="/public/img/category/yae.png" height="300" alt="Second slide">
      <div class="carousel-caption d-none d-md-block">
        <span class="Featured">{{second.CourseName}}</span>
        <br>
        <p class="rating-item">{{second.Rating}}</p>
        <i class="bi bi-star-fill"></i>
      </div>
        </div>
    </div>
    <div class="carousel-item">
        <div class="d-block w-100" style="text-align: center; background-color:black">
      <img src="/public/img/category/yae.png" height="300" alt="Third slide">
      <div class="carousel-caption d-none d-md-block">
        <span class="Featured">{{third.CourseName}}</span>
        <br>
        <p class="rating-item">{{third.Rating}}</p>
        <i class="bi bi-star-fill"></i>
      </div>
         </div>
    </div>
  </div>
  <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="sr-only">Previous</span>
  </a>
  <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="sr-only">Next</span>
  </a>
</div>
